{
  "name":        "aws-cloud-meta",
  "version":     "19.0.0",
  "description": "This meta-package can be used to limit AWS CDK versions to a specific version of the Cloud Assembly Schema.",
  "license":     "MIT",
  "homepage":    "https://github.com/Yogarine/aws-cloud-meta#readme",

  "scripts": {
    "cdk": "cdk"
  },

  "devDependencies": {
    "aws-cdk": "~2.25"
  },

  "peerDependencies": {
    "aws-cdk":            "~2.25",
    "aws-cdk-lib":        "~2.25",
    "constructs":         "^10",
    "source-map-support": "^0.5",

    "@types/jest":        "^27",
    "@types/node":        "^10.17",
    "jest":               "^27",
    "ts-jest":            "^27",
    "ts-node":            "^10",
    "typescript":         "^3.9"
  },

  "peerDependenciesMeta": {
    "aws-cdk":            { "optional": false },
    "aws-cdk-lib":        { "optional": false },
    "constructs":         { "optional": false },
    "source-map-support": { "optional": false },

    "@types/jest":        { "optional": true },
    "@types/node":        { "optional": true },
    "jest":               { "optional": true },
    "ts-jest":            { "optional": true },
    "ts-node":            { "optional": true },
    "typescript":         { "optional": true }
  }
}
